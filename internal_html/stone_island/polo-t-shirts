        </li></template><script type="module">import * as sdkImports from 'https://assets.xgen.dev/sdk/releases/v1.1.5-beta.0/xgen-sdk.min.js'; (async () => { try { const xgElemInfo = sdkImports.xgenElementInfo?.['sm_el_96b47db227334ef2a622121a40b39226'] || {}; const elementImports = { element: xgElemInfo.element, elementObject: xgElemInfo.elementObject, templates: xgElemInfo.templates, utils: sdkImports.utils, ell: sdkImports.ell, rerender: () => sdkImports.rerender(['sm_el_96b47db227334ef2a622121a40b39226']), fetchPredictions: sdkImports.fetchPredictions, renderTemplate: ({items, template, container, append}) => sdkImports.renderTemplate({ elementId: 'sm_el_96b47db227334ef2a622121a40b39226', items, template, container, append }),  }; xgElemInfo.element.removeAttribute('style'); (()=>{ if (/[?&]xse_debugger=(all|[^&]*sm_el_96b47db227334ef2a622121a40b39226[^&]*)/g.test(window.location.search)) debugger;})(); (async function() {
    const elementSelector = '#XSE-sm_el_96b47db227334ef2a622121a40b39226';
    const apiUrl = window.location.origin + '/yTos/api/Plugins/ItemPluginApi/GetCombinationsAsync/';

    /*tc_events_19 = function () {
        console.log('aaaa', arguments);
    }*/

    if (window.tc_vars?.product_discountprice != window.tc_vars?.product_price) {
        window.xgEll.dispatch('error-tracking', {
            track: true,
            message: 'Seems on sale, check',
            action: 'ON_SALE'
        });
        console.log('on Sale, stop');
        document.querySelector(elementSelector).style.display = 'none';
        return false;
    }

    const getProductColors = async (prodCode) => {
        const url = `${apiUrl}?siteCode=${yTos.siteCode()}&code10=${prodCode}`;
        const resp = await fetch(url);
        const json = await resp.json();
        return [json.Colors, json.Sizes];
    };

    function formatMoney(amount, decimalCount = 2, decimal = ".", thousands = ",") {
        try {
            decimalCount = Math.abs(decimalCount);
            decimalCount = isNaN(decimalCount) ? 2 : decimalCount;
            const negativeSign = amount < 0 ? "-" : "";
            let i = parseInt(amount = Math.abs(Number(amount) || 0).toFixed(decimalCount)).toString();
            let j = (i.length > 3) ? i.length % 3 : 0;
            return negativeSign + (j ? i.substr(0, j) + thousands : '') + i.substr(j).replace(/(\d{3})(?=\d)/g, "$1" + thousands) + (decimalCount ? decimal + Math.abs(amount - i).toFixed(decimalCount).slice(2) : "");
        } catch (e) {
            console.log(e);
        }
    };

    function formatNumber(format, price, currency) {
        let numberFormat = /[1-9].*[0-9]/.exec(format)[0];
        let milesSep = /[0-9]/.test(numberFormat[1]) ? '' : numberFormat[1];
        if (milesSep) {
            numberFormat = numberFormat[0] + numberFormat.substr(2);
        }
        numberFormat = numberFormat.substr(4);
        let decSep = '.';
        let numDec = 0;
        if (numberFormat[0]) {
            decSep = numberFormat[0];
            numDec = numberFormat.length - 1;
        }
        return format.replace('$', currency).replace(/[1-9].*[0-9]/, formatMoney(price, numDec, decSep, milesSep));
    }

    const onProducts = async (clbk) => {
        document.querySelectorAll(`${elementSelector} .xse_rec`).forEach(await clbk);
    };

    const combineVariants = (variants, colors) => {
        return colors.reduce((oldVal, newVal, index) => {
            oldVal[index] = {
                colorName: newVal.Description,
                colorHex: newVal.Rgb,
                image: '',
                link: newVal.Link,
                sizes: '',
                cod10: newVal.Code10
            };

            const consideredVariants = variants.filter((variant) => {
                return variant.variant_id.toLowerCase().includes(newVal.Code10.toLowerCase());
            });

            if (!consideredVariants.length) {
                return oldVal;
            }

            oldVal[index].sizes = consideredVariants.reduce((oldVal, variant) => {
                return oldVal + (oldVal.length ? '|' : '') + `${variant.size};${variant.is_in_stock}`;
            }, '');

            oldVal[index].image = consideredVariants[0].image;
            return oldVal;
        }, []);
    };

    const selectColor = (itm, color) => {
        itm.querySelectorAll('.xse_color_swatch').forEach((sw) => sw.classList.remove('selected'));
        color.classList.add('selected');
        const oldRef = itm.querySelector('a').getAttribute('href');
        itm.querySelector('a').setAttribute('href', color.getAttribute('data-link') + '?' + oldRef.split('?')[1]);
        itm.querySelector('img').setAttribute('src', color.getAttribute('data-img'));
        itm.dataset.selectedColorCode = color.dataset.cod;
    };

    let position = 0;

    onProducts(async (itm) => {
        const cod10 = itm.querySelector('a').href.split('_cod')[1].split('.html')[0].toUpperCase();
        itm.dataset.position = position;
        position++;
        const [colors, sizes] = await getProductColors(cod10);
        const variants = JSON.parse(itm.querySelector('.xse_variants').textContent.trim());
        const colorsCombined = combineVariants(variants, colors);

        itm.querySelector('a').addEventListener('click', () => {
            try {
                tc_events_19(itm, "enh_prod_xgen", {
                    "item_id": cod10,
                    "item_name": variants[0].prod_name,
                    "item_brand": 'Stone Island',
                    "item_category": JSON.parse(itm.querySelector('.xse_types').textContent.trim().toUpperCase()).slice(1).join('/'),
                    "item_list_name": "item|related_xgen",
                    "item_variant": variants[0].variant_id,
                    "price": itm.querySelector('.xse_prod_price').dataset.price,
                    "index": parseInt(itm.dataset.position),
                })
            } catch (e) {
                console.log(e);
            }

        });

        try {
            tc_events_19(itm, "related_impressions_xgen", {
                "item_id": cod10,
                "item_name": variants[0].prod_name,
                "item_brand": 'Stone Island',
                "item_category": JSON.parse(itm.querySelector('.xse_types').textContent.trim().toUpperCase()).slice(1).join('/'),
                "item_list_name": "item|related_xgen",
                "item_variant": variants[0].variant_id,
                "price": itm.querySelector('.xse_prod_price').dataset.price,
                "index": parseInt(itm.dataset.position),
            })
        } catch (e) {
            console.log(e);
        }

        itm.querySelector('.xse_swatch').addEventListener('click', (evt) => {
            if (window.innerWidth < 768) {
                evt.preventDefault();
                evt.stopPropagation();
                itm.querySelector('.xse_swatch').style.transform = 'translate(0,100%)';
            }
        });

        let missingColors = 0;
        let colorsHtml = colorsCombined.reduce((oldVal, color, index) => {
            if (index <= (3 + missingColors)) {
                let sclassName = 'xse_color_swatch';
                if (!color.image) {
                    missingColors += 1;
                    sclassName += ' hidden-color';
                }
                return oldVal + `<div class="${sclassName}" style="background-color: #${color.colorHex};" title="${color.colorName}" data-cod="${color.cod10}" data-sizes="${color.sizes}" data-img="${color.image}" data-link="${color.link}"></div>`
            } else if (index == (4 + missingColors)) {
                const otherColors = colorsCombined.length - 4;
                return oldVal + `<span class="xse_more_colors">+ ${otherColors}</span>` + `<div class="xse_color_swatch hidden-color" style="background-color: #${color.colorHex};" title="${color.colorName}" data-cod="${color.cod10}" data-sizes="${color.sizes}" data-img="${color.image}" data-link="${color.link}"></div>`;
            } else {
                return oldVal + `<div class="xse_color_swatch hidden-color" style="background-color: #${color.colorHex};" title="${color.colorName}" data-cod="${color.cod10}" data-sizes="${color.sizes}" data-img="${color.image}" data-link="${color.link}"></div>`;
            }
        }, '');

        if (missingColors > 0 && colorsCombined.length < (4 + missingColors)) colorsHtml += `<span class="xse_more_colors">+ ${missingColors}</span>`;

        itm.querySelector('.xse_colors').innerHTML = colorsHtml;

        if (itm.querySelector('.xse_more_colors')) {
            itm.querySelector('.xse_more_colors').addEventListener('click', function(evt) {
                evt.preventDefault();
                evt.stopPropagation();
                this.remove();
                itm.querySelectorAll('.xse_color_swatch.hidden-color').forEach((clr) => {
                    clr.classList.remove('hidden-color');
                });
            });
        }

        itm.querySelectorAll('.xse_color_swatch').forEach((tsw) => {
            tsw.addEventListener('click', function(evt) {
                evt.preventDefault();
                evt.stopPropagation();
                selectColor(itm, evt.target, sizes);
            });
            if (itm.querySelector('a').href.toLowerCase().includes(tsw.getAttribute('data-cod').toLowerCase())) {
                selectColor(itm, tsw, sizes);
            }
        });

        if (!itm.querySelector('.xse_color_swatch.selected')) {
            selectColor(itm, itm.querySelector('.xse_color_swatch'), sizes);
        }

        const priceField = itm.querySelector('.xse_prod_price');
        const salePriceField = itm.querySelector('.xse_prod_sale_price');

        priceField.innerText = formatNumber(priceField.dataset.format, priceField.dataset.price, priceField.dataset.currency);
        salePriceField.innerText = formatNumber(salePriceField.dataset.format, salePriceField.dataset.price, salePriceField.dataset.currency);

        if (priceField.textContent != salePriceField.textContent) {
            salePriceField.style.display = 'inline';
            priceField.style.textDecoration = 'line-through';
            const price = parseFloat(priceField.dataset.price);
            const sale = parseFloat(salePriceField.dataset.price);
            const percent = 100 - Math.round(((sale / price) * 100) / 10) * 10;
            itm.querySelector('.xse_price_off').textContent = percent + '% OFF';
        }

        /*sizes.forEach(size => {
            const sizeNode = document.createElement('div');
            sizeNode.className = 'xse_size selectSize sizeWSelection';
            sizeNode.innerText = size.Description.toUpperCase();
            if (variants.find(variant => variant.size = size.Description).is_in_stock != 'in stock') {
                sizeNode.classList.add('disabled');
            }
            itm.querySelector('.xse_sizes').appendChild(sizeNode);
        });*/
    });


    const element = document.querySelector(elementSelector);
    const filterNode = document.querySelector('#recapContainer');

    // if filters are set on page load, hide element
    const isMobile = document.documentElement.classList.contains('smartphone'); // Check if we're on the mobile layout
    const isHidden = filterNode.classList.contains('hidden'); //Desktop
    const isVisible = filterNode.classList.contains('visible'); //Mobile

    if ((!isHidden && !isMobile) || (isMobile && isVisible)) element.style.display = 'none'; //check if filters are already applied

    const observer = new MutationObserver((mutationList, observer) =>
        mutationList.forEach(mutation => {
            element.style.display = ((filterNode.classList.contains('hidden') || (isMobile && !filterNode.classList.contains('visible'))) ? null : 'none') //hide/display carousel when filters change
        })
    );
    observer.observe(filterNode, {
        attributes: true
    });

})(); } catch (err) { console.error(err); } })();</script></div>

        </div>

    </main><iframe style="width: 0px; height: 0px; display: none;"></iframe>




